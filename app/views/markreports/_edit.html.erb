<div class="modal-header">
  <h3>Edit Mark List</h3>
</div>
<%= form_for @mark, :url=>class_markreport_path(@school_name.school_name,:class_id => @class_id,:id=>@mark.id), :html=> { :class=> "form-horizontal", :style=> "display:inline;" } do |f| %> 
  <%= hidden_field_tag :user_id,@user_id %>
  <%= hidden_field_tag :school_admin_id,@school_admin_id %>
  <%= hidden_field_tag :class_id,@class_id %>
  <%= hidden_field_tag :subject_id,@subject_id %>
  <%= hidden_field_tag :receiver_id,@receiver_id %>
  <%= render :partial=>'form',:locals => {:f=>f} %>
<% end %>