<div class="modal-body">
  <ul class="errors"></ul>
  <%= hidden_field_tag :user_id,@user_id %>
  <%= hidden_field_tag :school_admin_id,@school_admin_id %>
  <%= hidden_field_tag :class_id,@class_id %>
  <%= hidden_field_tag :subject_id,@subject_id %>
  <%= hidden_field_tag :receiver_id,@receiver_id %>

  <div class="control-group">
    <%= f.label :pass_marks, class:"control-label" %>
      <div class="controls">
        <%= f.text_field :min_marks %>
      </div>
    </div>
    <div class="control-group">
      <%= f.label :marks_obtained, class: "control-label" %>
        <div class="controls">
          <%= f.text_field :max_marks %>
        </div>
      </div>
      <div class="control-group">
        <%= f.label :total_marks, class: "control-label" %>
          <div class="controls">
            <%= f.text_field :total_marks %>
          </div>
        </div>
        <div class="control-group">
          <%= f.label :comment, class: "control-label" %>
            <div class="controls">
              <%= f.text_area :comment,:rows=>2,:cols=>2 %>
            </div>
          </div>
          <div class="control-group">
            <%= f.label :marksheet_upload, class: "control-label" %>
              <div class="controls">
                <%= f.file_field :markreport_document%>
              </div>
            </div>
          </div>
          <div class="modal-footer">
            <%= f.submit class: "btn btn-primary" %>
              <%= link_to "Cancel", "#", class: "btn", data: {dismiss: "modal"} %>
              </div>



