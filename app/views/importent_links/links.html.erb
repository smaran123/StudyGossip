<div class="popup_post_box responsive_height" style="min-height: 200px;">
  <div style="margin: -14px 8px;float :right;">
    <%=image_tag("simple-overlay-close.gif", :style => "cursor: pointer;margin : 21px -5px -2px 0px;", :border =>0,:width => '20px', :height => '20px', :onclick => "hide_popup();")%>
  </div>
  <div class="whats_happening">Important Links</div>
  <% if @links.present? %>
    <% for link in @links %>
      <div class="faqs_list" style="text-align: left;">
        <div class="faq_title">
          <%= link.title %>?
        </div>
      </div><br />
    <% end %>
  <% else %>
    <span style="color: red;position: relative;top: 60px;">No Importent Links</span>
  <% end %>
</div>